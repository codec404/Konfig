{
  "service": "payment-service",
  "environment": "production",
  "settings": {
    "max_connections": 100,
    "timeout_ms": 5000,
    "retry_attempts": 3,
    "log_level": "info",
    "features": {
      "fraud_detection": true,
      "instant_refunds": false,
      "multi_currency": true
    },
    "database": {
      "host": "payment-db.internal",
      "port": 5432,
      "pool_size": 25
    }
  }
}
